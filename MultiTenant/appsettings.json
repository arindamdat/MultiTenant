{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "Serilog": {
    "MinimumLevel": {
      "Default": "Debug",
      "Override": {
        "Microsoft": "Debug",
        "System": "Debug"
      }
    },
    "WriteTo": [
      { "Name": "Console" }
    ]
  },
  "Keycloak": {
    "Url": "http://keycloak.local:8080",
    "AdminUser": "admin",
    "AdminPassword": "adminpass",
    "RequireHttps": false
  }
}
